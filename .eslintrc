{
  "extends": "airbnb",
  "root": true,
  "env": {
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "id-length": 0,
    "new-cap": [2, { "capIsNewExceptions": ["AND"] }],
    "react/jsx-pascal-case": [2, { "allowAllCaps": true }],
    "no-underscore-dangle": [2, {
      "allowAfterThis": true
    }]
  }
}